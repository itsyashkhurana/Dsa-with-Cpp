#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cin>>a>>b>>c;
    if(a>b){ // if fail means b>a
        if(a>c){  // if fail means c>a>b
            cout<<" a  ";
        }
        else { 
            cout<<" c ";
        }
    }

    else{ // b>a
        if(b>c){ // if fail means c>b>a
            cout<<"b";
        }
        else {
            cout<<"c ";
        }
    }
}
